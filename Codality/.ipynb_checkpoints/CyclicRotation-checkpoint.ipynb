    "def solution(A,K):\n",
    "    if len(A) < 2:\n",
    "        return A\n",
    "    if K < 1:\n",
    "        return A\n",
    "\n",
    "    K = K % len(A)\n",
    "    left_part = A[:-K]\n",
    "    right_part = A[-K:]\n",
    "    return right_part + left_part"
   ]
