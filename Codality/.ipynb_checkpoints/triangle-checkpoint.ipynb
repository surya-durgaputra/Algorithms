   "source": [
    "def solution(A):\n",
    "    A = list(set(A))\n",
    "    if (len(A)<3):\n",
    "        return 0\n",
    "    \n",
    "    A = sorted(A)\n",
    "    for index in range(0,len(A)-2):\n",
    "        if A[index] + A[index+1]>A[index+2]:\n",
    "            return 1\n",
    "    \n",
    "    return 0"
   ]
